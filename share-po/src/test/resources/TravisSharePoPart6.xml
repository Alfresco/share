<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Alfresco Share Page Object Tests" verbose="1" parallel="classes">
    <groups>
        <run>
            <exclude name="bug"/>
            <exclude name="chromeOnly"/>
            <exclude name="Hybrid"/>
            <exclude name="download"/>
            <exclude name="NonGrid"/>
