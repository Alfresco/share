<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Download Suite" verbose="6" preserve-order="true" parallel="classes" thread-count="1">
    <groups>
        <run>
            <exclude name="bug"/>
            <exclude name="chromeOnly"/>
            <exclude name="Hybrid"/>
            <exclude name="download"/>
            <exclude name="NonGrid"/>
            <exclude name="Cloud-only"/>
        </run>
    </groups>

    <test name="DownloadFileAndFolderTest">
        <classes>
            <class name="org.alfresco.po.share.site.document.download.DownloadFileAndFolderTest" />
        </classes>
    </test>
</suite>