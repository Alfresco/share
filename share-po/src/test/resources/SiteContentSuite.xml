<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Site Content Suite" verbose="6" preserve-order="true" parallel="classes" thread-count="3">
    <groups>
        <run>
            <exclude name="bug"/>
            <exclude name="chromeOnly"/>
            <exclude name="Hybrid"/>
            <exclude name="download"/>
            <exclude name="NonGrid"/>
            <exclude name="Cloud-only"/>
        </run>
    </groups>

    <test name="SimpleViewInformationTest">
        <classes>
            <class name="org.alfresco.po.share.dashlet.sitecontent.SimpleViewInformationTest" />
        </classes>
    </test>
</suite>