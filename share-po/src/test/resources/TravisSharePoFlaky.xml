        <classes>
            <class name="org.alfresco.po.share.user.UserSitesPageTest" />
        </classes>
    </test>

    <test name="CustomizeSiteDashboardPageTest">
        <classes>
            <class name="org.alfresco.po.share.site.CustomizeSiteDashboardPageTest" />
        </classes>
    </test>

    <test name="FacetedSearchBulkActionsE2ETest">
        <classes>
