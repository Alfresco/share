<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="app:applicationmodel">
    <description>Alfresco Application Model</description>
    <author>Alfresco</author>
    <published>2005-09-29</published>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
    </imports>
    <namespaces>
        <namespace uri="http://www.alfresco.org/model/application/1.0" prefix="app"/>
    </namespaces>
    <data-types/>
    <constraints/>
    <types>
        <type name="app:glossary">
            <parent>cm:folder</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="app:configurations">
            <title>Configurations</title>
            <parent>cm:systemfolder</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="app:filelink">
            <title>File Link Object</title>
            <parent>cm:link</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="app:folderlink">
            <title>Folder Link Object</title>
            <parent>cm:link</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
    </types>
    <aspects>
        <aspect name="app:uifacets">
            <title>UI Facets</title>
            <parent>cm:titled</parent>
            <properties>
                <property name="app:icon">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="app:inlineeditable">
            <title>Inline Editable</title>
            <properties>
                <property name="app:editInline">
                    <title>Edit Inline</title>
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="app:workflow">
            <title>Workflow</title>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="app:simpleworkflow">
            <parent>app:workflow</parent>
            <properties>
                <property name="app:approveStep">
                    <type>d:text</type>
                    <protected>true</protected>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="app:approveFolder">
                    <type>d:noderef</type>
                    <protected>true</protected>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="app:approveMove">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="app:rejectStep">
                    <type>d:text</type>
                    <protected>true</protected>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="app:rejectFolder">
                    <type>d:noderef</type>
                    <protected>true</protected>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="app:rejectMove">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="app:configurable">
            <title>Configurable</title>
            <properties/>
            <associations>
                <child-association name="app:configurations">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>app:configurations</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>false</many>
                    </target>
                </child-association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="app:feedsource">
            <title>Feed Source</title>
            <properties>
                <property name="app:template">
                    <title>Feed Template</title>
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="app:defaultViewConfig">
            <title>Default View Config</title>
            <properties>
                <property name="app:defaultViewId">
                    <title>Default View Id</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
    </aspects>
</model>