<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="sys:systemmodel">
    <description>Alfresco Repository System Definitions</description>
    <author>Alfresco</author>
    <published>2005-09-29</published>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
    </imports>
    <namespaces>
        <namespace uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
        <namespace uri="http://www.alfresco.org/system/registry/1.0" prefix="reg"/>
        <namespace uri="http://www.alfresco.org/system/modules/1.0" prefix="module"/>
    </namespaces>
    <data-types/>
    <constraints/>
    <types>
        <type name="sys:deleted">
            <title>Deleted</title>
            <description>Placeholder type for deleted nodes</description>
            <properties>
                <property name="sys:originalId">
                    <type>d:long</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sys:base">
            <title>Base</title>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects>
                <aspect>sys:referenceable</aspect>
                <aspect>sys:localized</aspect>
            </mandatory-aspects>
        </type>
        <type name="sys:descriptor">
            <title>Descriptor</title>
            <parent>sys:base</parent>
            <properties>
                <property name="sys:name">
                    <type>d:text</type>
                    <mandatory enforced="true">true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionMajor">
                    <type>d:text</type>
                    <mandatory enforced="true">true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionMinor">
                    <type>d:text</type>
                    <mandatory enforced="true">true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionRevision">
                    <type>d:text</type>
                    <mandatory enforced="true">true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionLabel">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:licenseMode">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionBuild">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionSchema">
                    <type>d:int</type>
                    <mandatory enforced="true">true</mandatory>
                    <default>0</default>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionEdition">
                    <type>d:content</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:versionProperties">
                    <type>d:content</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sys:container">
            <title>Container</title>
            <parent>sys:base</parent>
            <properties/>
            <associations>
                <child-association name="sys:children">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>sys:base</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                </child-association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sys:store_root">
            <title>Store Root</title>
            <parent>sys:container</parent>
            <properties/>
            <associations>
                <child-association name="sys:lost_found">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>sys:lost_found</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>false</many>
                    </target>
                </child-association>
            </associations>
            <overrides/>
            <mandatory-aspects>
                <aspect>sys:aspect_root</aspect>
            </mandatory-aspects>
        </type>
        <type name="sys:reference">
            <title>Reference</title>
            <parent>sys:base</parent>
            <properties>
                <property name="sys:reference">
                    <type>d:noderef</type>
                    <mandatory enforced="true">true</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sys:lost_found">
            <title>Lost+Found</title>
            <parent>sys:container</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="sys:archiveUser">
            <title>Archive User</title>
            <description>Type to gather all nodes archived by a user</description>
            <parent>sys:base</parent>
            <properties/>
            <associations>
                <child-association name="sys:archivedLink">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>sys:archived</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                </child-association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </type>
    </types>
    <aspects>
        <aspect name="sys:aspect_root">
            <title>Root</title>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:referenceable">
            <title>Referenceable</title>
            <properties>
                <property name="sys:store-protocol">
                    <type>d:text</type>
                    <mandatory enforced="true">true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:store-identifier">
                    <type>d:text</type>
                    <mandatory enforced="true">true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:node-uuid">
                    <type>d:text</type>
                    <mandatory enforced="true">true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:node-dbid">
                    <type>d:long</type>
                    <mandatory enforced="true">true</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:incomplete">
            <title>Incomplete</title>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:temporary">
            <title>Temporary</title>
            <archive>false</archive>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:hidden">
            <title>Hidden</title>
            <archive>false</archive>
            <properties>
                <property name="sys:clientVisibilityMask">
                    <type>d:int</type>
                    <mandatory>false</mandatory>
                    <default>0</default>
                    <index enabled="true"/>
                </property>
                <property name="sys:hiddenFlag">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:cascadeHidden">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <default>false</default>
                    <index enabled="true"/>
                </property>
                <property name="sys:cascadeIndexControl">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <default>false</default>
                    <index enabled="true"/>
                </property>
                <property name="sys:clientControlled">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:undeletable">
            <title>Undeletable</title>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:pendingDelete">
            <title>Pending Delete</title>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:noContent">
            <title>NoContent</title>
            <archive>false</archive>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:webdavNoContent">
            <title>NoContent</title>
            <archive>false</archive>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:archiveRoot">
            <title>Archive Root</title>
            <description>Aspect attached to root of archive store</description>
            <properties/>
            <associations>
                <child-association name="sys:archiveUserLink">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>sys:archiveUser</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                    <duplicate>false</duplicate>
                </child-association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:archived">
            <title>Archived</title>
            <properties>
                <property name="sys:archivedOriginalParentAssoc">
                    <type>d:childassocref</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:archivedBy">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sys:archivedDate">
                    <type>d:datetime</type>
                    <mandatory>true</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>BOTH</tokenised>
                    </index>
                </property>
                <property name="sys:archivedOriginalOwner">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:archived-assocs">
            <properties>
                <property name="sys:archivedParentAssocs">
                    <type>d:childassocref</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                    <index enabled="false"/>
                </property>
                <property name="sys:archivedChildAssocs">
                    <type>d:childassocref</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                    <index enabled="false"/>
                </property>
                <property name="sys:archivedSourceAssocs">
                    <type>d:assocref</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                    <index enabled="false"/>
                </property>
                <property name="sys:archivedTargetAssocs">
                    <type>d:assocref</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                    <index enabled="false"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:localized">
            <title>Translation</title>
            <properties>
                <property name="sys:locale">
                    <title>Locale</title>
                    <type>d:locale</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:softDelete">
            <title>SoftDelete</title>
            <archive>false</archive>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sys:CMISUpdateContext">
            <title>CMIS Update Context</title>
            <properties>
                <property name="sys:gotFirstChunk">
                    <title>Got First Content Chunk</title>
                    <type>d:boolean</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
    </aspects>
</model>