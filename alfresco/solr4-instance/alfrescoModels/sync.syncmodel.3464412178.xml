<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="sync:syncmodel">
    <description>Alfresco Sync Domain Model</description>
    <author>Alfresco</author>
    <published>2012-05-23</published>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
    </imports>
    <namespaces>
        <namespace uri="http://www.alfresco.org/model/sync/1.0" prefix="sync"/>
    </namespaces>
    <data-types/>
    <constraints/>
    <types>
        <type name="sync:syncSetDefinition">
            <parent>cm:cmobject</parent>
            <archive>false</archive>
            <properties>
                <property name="sync:syncGuid">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:sourceRepoId">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:syncCreatorPerson">
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:syncCreatorUsername">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:targetNetworkId">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:targetRootFolder">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:syncSetIsLockedOnPremise">
                    <type>d:boolean</type>
                    <mandatory>true</mandatory>
                    <default>false</default>
                    <index enabled="true"/>
                </property>
                <property name="sync:includeSubFolders">
                    <type>d:boolean</type>
                    <mandatory>true</mandatory>
                    <default>false</default>
                    <index enabled="true"/>
                </property>
                <property name="sync:isDeleteOnCloud">
                    <type>d:boolean</type>
                    <mandatory>true</mandatory>
                    <default>true</default>
                    <index enabled="true"/>
                </property>
                <property name="sync:isDeleteOnPrem">
                    <type>d:boolean</type>
                    <mandatory>true</mandatory>
                    <default>true</default>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations>
                <association name="sync:members">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>sync:syncSetMemberNode</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </type>
    </types>
    <aspects>
        <aspect name="sync:syncSetMemberNode">
            <properties>
                <property name="sync:otherNodeRefString">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:remoteModified">
                    <title>Remote Modified</title>
                    <type>d:datetime</type>
                    <protected>true</protected>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>BOTH</tokenised>
                    </index>
                </property>
                <property name="sync:remoteModifier">
                    <title>Remote Modifier</title>
                    <type>d:text</type>
                    <protected>true</protected>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:syncRequested">
                    <title>Has a sync been requested </title>
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <default>false</default>
                    <index enabled="true"/>
                </property>
                <property name="sync:directSync">
                    <title>Direct/Indirect Sync </title>
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <default>false</default>
                    <index enabled="true"/>
                </property>
                <property name="sync:syncOwner">
                    <title>Username (on Source Repo) of Sync Set Owner</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:syncTime">
                    <type>d:datetime</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:syncLock">
                    <title>Was this node locked by sync?</title>
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <default>false</default>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sync:synced">
            <properties>
                <property name="sync:thisVersionLabel">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:remoteVersionLabel">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sync:failed">
            <properties>
                <property name="sync:errorCode">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:errorDetails">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="sync:errorTime">
                    <type>d:datetime</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sync:transientError">
            <properties>
                <property name="sync:transientErrorCode">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="sync:deleteOnPrem">
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
    </aspects>
</model>